{
    "manifest_version": 3,
    "name": "Website Tracker Extension",
    "version": "1.0",
    "description": "Tracks the websites visited by the user and sends data to the backend",
    "permissions": [
      "activeTab",
      "storage",
      "tabs",
      "background"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
  }
  